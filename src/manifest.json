{
  "manifest_version": 3,
  "name": "Kur0's Tab Purger",
  "version": "4.2.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "description": "A tab manager that lets you purge the search results with one click!",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
    "action": {
    "default_popup": "index.html"
  }
}